                <criterion comment="Test that a regkeyauditedpermissions_object with a hive, key pattern match, and trustee_name is supported." test_ref="oval:org.mitre.oval.test:tst:1218"/>
                <criterion comment="Test that the max_depth behavior is supported for limited depth of recursion." test_ref="oval:org.mitre.oval.test:tst:1118"/>
                <criterion comment="Test that the max_depth behavior is supported for unlimited depth of recursion." test_ref="oval:org.mitre.oval.test:tst:1619"/>
                <criterion comment="Test that the recurse_direction behavior is supported in the 'up' direction." test_ref="oval:org.mitre.oval.test:tst:1917"/>
                <criterion comment="Test that the include_group behavior is supported." test_ref="oval:org.mitre.oval.test:tst:1269"/>
                <criterion comment="Test that the resolve_group behavior is supported." test_ref="oval:org.mitre.oval.test:tst:1802"/>
